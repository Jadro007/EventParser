/* ------------------------------------ /modules/globalcontent/globalcontent-web-select2.js ------------------------------------ */


var mappengine = mappengine || {};

/**
 * Jmenný prostor pro webové JS API modulu Globalcontent určený pro výměnu obsahu.
 * @namespace
 */
mappengine.globalcontent = mappengine.globalcontent || {};

mappengine.globalcontent.select2 = {
    InitOnReady: function() {
        $(".select2Single").select2({
            language: "cs",
            theme: "cgfFilter",
            minimumResultsForSearch: Infinity
        });
        $(".select2Multiple").select2({
            language: "cs",
            theme: "cgfFilter"
        });
    }
};

$(document).ready( $.proxy( mappengine.globalcontent.select2.InitOnReady, mappengine.globalcontent.select2 ) );

/* ------------------------------------ /modules/globalcontent/globalcontent-web-datepicker.js ------------------------------------ */


var mappengine = mappengine || {};

/**
 * Jmenný prostor pro webové JS API modulu Globalcontent určený pro výměnu obsahu.
 * @namespace
 */
mappengine.globalcontent = mappengine.globalcontent || {};

mappengine.globalcontent.datepicker = {
    InitOnReady: function() {
        $("input.datepickerSelect").datepicker();
    }
};

$(document).ready( $.proxy( mappengine.globalcontent.datepicker.InitOnReady, mappengine.globalcontent.datepicker ) );

/* ------------------------------------ /inc/momentjs/momentjs-2.18.1/moment.min.js ------------------------------------ */

//! moment.js
//! version : 2.18.1
//! authors : Tim Wood, Iskren Chernev, Moment.js contributors
