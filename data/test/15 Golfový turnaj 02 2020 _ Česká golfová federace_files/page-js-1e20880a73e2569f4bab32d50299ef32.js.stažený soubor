  function WEBUSER_GetLostPassword(glp_page)
  {
    var mywin  = window.open(glp_page,"","width=300,height=140,scrollbars=yes,status=no,toolbar=no,menubar=no,location=no,resizable=no");
    mywin.focus();
  }

