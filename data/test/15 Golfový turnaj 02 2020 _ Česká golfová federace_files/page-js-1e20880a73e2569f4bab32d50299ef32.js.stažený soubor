// -----------------------------------------------------------------------------
// LOGGED BOX
// -----------------------------------------------------------------------------

  $(document).ready( function() {
  
    $("#logged-box").mouseover(function(){
       loggedBox.showActions();
    });
    
    $("#logged-box").mouseleave(function(){
       loggedBox.hideActions();
    });



  });
  
  
  var loggedBox = {};
  
  loggedBox.showActions = function()
  {
  
   $("#logged-box-actions").animate(
      {
          width: "show",
          opacity: "show"
      },
      200
    );

  }
  
  loggedBox.hideActions = function()
  {
    $("#logged-box-actions").animate(
      {
          width: "hide",
          opacity: "hide"
      },
      200
    );
  }


/* ------------------------------------ /cz/js-version-2020.10.25-2.js ------------------------------------ */


/* ------------------------------------ /inc/mappengine/mappengine.js ------------------------------------ */


/**
 * Základní jmenný prostor všech balíčků MAppEngine JavaScript API.
 * @namespace
 */
var mappengine = mappengine || {};

mappengine.net = mappengine.net || {};
mappengine.net.cookies = mappengine.net.cookies || {};

mappengine.utils = mappengine.utils || {};
mappengine.utils.array = mappengine.utils.array || {};
mappengine.utils.string = mappengine.utils.string || {};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Vrátí hodnotu cookie.
 * @param {String} name Jméno požadované cookie.
 * @returns {String}
 */
mappengine.net.cookies.getCookie = function(name) {
    var cookiestring = "" + document.cookie;
    var index1 = cookiestring.indexOf(name);
    if (index1===-1) { return ""; }
    var index2 = cookiestring.indexOf(';',index1);
    if (index2===-1) { index2=cookiestring.length; }
    return unescape(cookiestring.substring(index1+name.length+1,index2));
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Nastaví hodnotu cookie. 
 * @param {String} name Jméno cookie.
 * @param {String} value Hodnota cookie.
 * @param {String} expires Datum a čas expirace ve formátu UTC time.
 * @param {String} path URL ke které cookie náleží.
 */
mappengine.net.cookies.setCookie = function(name, value, expires, path) {
    
    // hodnota value by měla být minimálně escapována pomocí funkce escape() nebo encodeURIComponent()
    value = escape(value); 
    
    // expirace cookie
    if (expires) { value+=';expires=' + expires; }
    
    // url cookie
    if (path) { value+=';path=' + path; }

    // uložíme cookie do dokumentu
    document.cookie = name + '=' + value;
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Vrací datum a čas ve formátu GMTString.
 * @param {Number} days Počet dní které budou přičteny k aktuálnímu času.
 * @returns {String}
 */
mappengine.net.cookies.getExpires = function(days) {
    days = days || 0;
    var date = new Date();
    date.setTime(date.getTime() + (days*24*60*60*1000)); 
    return date.toGMTString();
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Odstraní z pole duplikáty a vrátí vyfiltrované pole, 
 * původní pole ponechá beze změn.
 * @param {Array} array
 * @returns {Array}
 */
mappengine.utils.array.unique = function(array) {
    var result = [];
    array.forEach(function(item) {
        if (result.indexOf(item) < 0) {
            result.push(item);
        }
    });
    return result;
};

/**
 * Porovná pole array1 proti poli array2 a vrací hodnoty z pole array1, které nejsou přítomny poli array2.
 * @param {Array} array1
 * @param {Array} array2
 * @returns {Array}
 */
mappengine.utils.array.diff = function(array1, array2) {
    var result = [];
    array1.forEach(function(item) {
        if (array2.indexOf(item) < 0) {
            result.push(item);
        }
    });
    return result;
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Vrací datum ve formátu yyyy-mm-dd.
 * @param {Date} dateObj
 * @returns {String}
 */
mappengine.utils.string.getDbDate = function (dateObj) {
    if (!dateObj) return "";

    var year  = dateObj.getFullYear();
    var month = dateObj.getMonth()+1;
    var day   = dateObj.getDate();
    var str   = year.toString();
        str  += "-" + (month<10?"0":"") +  month.toString();
        str  += "-" + (day<10?"0":"") +  day.toString();
        
    return str; 
};

/**
 * Převede datum z formátu yyyy-mm-dd do formátu d[d].m[m].yyyy.
 * @param {String} dbDate
 * @returns {String}
 */
mappengine.utils.string.formatDate = function (dbDate) {
    if (!dbDate) return "";

    var date_arr = dbDate.split("-");
    var year  = date_arr[0];
    var month = date_arr[1];
    var day   = date_arr[2];

    if (month[0] === "0")  month  = month[1];
    if (day[0] === "0")    day  = day[1];

    var str = day + "." + month + "." + year;
    return  str;
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/* ------------------------------------ /inc/jquery/jquery-ui-localization/jquery.ui.datepicker-cs.js ------------------------------------ */

/* Czech initialisation for the jQuery UI date picker plugin. */
