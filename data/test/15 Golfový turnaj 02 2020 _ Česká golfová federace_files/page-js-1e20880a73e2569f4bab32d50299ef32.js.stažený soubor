/* ------------------------------------ /modules/webhierarchy/webhierarchy-web.js ------------------------------------ */


var mappengine = mappengine || {};
mappengine.web = mappengine.web || {};

/**
 * Jmenný prostor pro webové JS API modulu Webhierarchy.
 * @namespace
 */
mappengine.web.webhierarchy = {};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Provede aktualizaci rozměrů responzivních iframů.
 * @public
 * @static
 */
mappengine.web.webhierarchy.updateResponsiveIframes = function() {
    var iframeList = $(".responsiveIframe");
    if (iframeList && iframeList.length>0) {
        for (var i=0; i<iframeList.length; i++) {
            mappengine.web.webhierarchy.updateIframe(iframeList[i]);
        }
    }
};

/**
 * Provede aktualizaci rozměrů responzivního iframu.
 * @public
 * @static
 * @param {String} iframeElement
 */
mappengine.web.webhierarchy.updateIframe = function(iframeElement) {
    var w = $(iframeElement).attr("width");
    var h = $(iframeElement).attr("height");

    $(iframeElement).width("100%");

    if(isFinite(w) && isFinite(h)) {
        $(iframeElement).height($(iframeElement).width() * h / w);
    } else {
        $(iframeElement).outerWidth("100%");
        $(iframeElement).outerHeight("100%");
    }
};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------


$(document).ready(function(){
    mappengine.web.webhierarchy.updateResponsiveIframes();
    $(window).resize(mappengine.web.webhierarchy.updateResponsiveIframes);
});

