!function(a){"object"==typeof module&&module.exports?module.exports=a(require("jquery")):a(window.jQuery)}(function(a){function b(a,b,c){var d=c[a];return void 0===d?c[a]=[b]:d.push(b),c}function c(c,d){var e={},f=c.map(function(){return this.elements?a.makeArray(this.elements):this}).filter(d||":input:not(:disabled)").get();return a.each(f,function(a,c){b(c.name,c,e)}),e}function d(a){return(a.type||a.nodeName).toLowerCase()}function e(c){var d={},e=/\+/g;return a.isPlainObject(c)?(a.extend(d,c),a.each(d,function(b,c){a.isArray(c)||(d[b]=[c])})):a.isArray(c)?a.each(c,function(a,c){b(c.name,c.value,d)}):"string"==typeof c&&a.each(c.split("&"),function(a,c){var f=c.split("=");b(decodeURIComponent(f[0].replace(e,"%20")),decodeURIComponent(f[1].replace(e,"%20")),d)}),d}function g(b){var c=d(b),e=void 0;return a.each(f,function(b,d){if(a.inArray(c,d)>-1)return e=b,!1}),e}function h(b,c,d,e,f){var h=g(b);if("value"==h&&c==e)b.value=d,f.call(b,d);else if("checked"==h||"selected"==h){var i=[];b.options?a.each(b.options,function(a,b){i.push(b)}):i.push(b),b.multiple&&0==e&&(b.selectedIndex=-1),a.each(i,function(a,b){b.value==d&&(b[h]=!0,f.call(b,d))})}}var f={checked:["radio","checkbox"],selected:["option","select-one","select-multiple"],value:["button","color","date","datetime","datetime-local","email","hidden","month","number","password","range","reset","search","submit","tel","text","textarea","time","url","week"]},i={change:a.noop,complete:a.noop};a.fn.deserialize=function(b,d){a.isFunction(d)&&(d={complete:d}),d=a.extend(i,d||{}),b=e(b);var f=c(this,d.filter);return a.each(b,function(b,c){a.each(f[b],function(b,e){a.each(c,function(a,c){h(e,b,c,a,d.change)})})}),d.complete.call(this),this}});

/* ------------------------------------ /modules/globalcontent/globalcontent-web-mainmenu.js ------------------------------------ */


var mappengine = mappengine || {};

/**
 * Jmenný prostor pro webové JS API modulu Globalcontent.
 * @namespace
 */
mappengine.globalcontent = mappengine.globalcontent || {};

// -----------------------------------------------------------------------------------------------------------------------------------------
// -----------------------------------------------------------------------------------------------------------------------------------------

/**
 * Hlavní menu.
 * @type Object
 */
mappengine.globalcontent.mainmenu = {
    currentMobileId: 0,
    currentDesktopId: 0,
    openedDesktopId: 0,
    mobileWidth: 991,
    openDelay: 50,
    openTimer: null
};

/**
 * Provede inicializaci hlavního menu.
 */
mappengine.globalcontent.mainmenu.init = function() {
    
    var _this = mappengine.globalcontent.mainmenu;

    $("a.link_disabled").on("click", function(e){e.preventDefault(); return false;});

    // tlačítka hlavního menu - mobile
    $(".mainmenu.mainmenu-level-1 > .mainmenu-button > a.mainmenu-item").click(function(event){
        var width = $(window).width();
        if (width <= _this.mobileWidth) {
            var dataItemId = $(event.currentTarget.parentNode).attr("data-item-id");
            var content = $(".mainmenu .mainmenu-level-2[data-item-id="+dataItemId+"]");
            if( content.length > 0 ) {
                event.preventDefault();
                event.stopPropagation();
                _this.switchMobileSubmenu($(event.currentTarget.parentNode).attr("data-item-id"));
            }
        }
    });
    
    // tlačítka hlavního menu - desktop
    $(".mainmenu .mainmenu-button").mouseenter(function(event){
        var width = $(window).width();
        if (width > _this.mobileWidth) {
            if (_this.hasDesktopSubmenu($(event.currentTarget).attr("data-item-id"))) {
                _this.startDesktopTimer($(event.currentTarget).attr("data-item-id"));
            } else {
                _this.closeDesktopSubmenu();
                _this.unhighlightDesktopOpenedItem();
                _this.currentDesktopId = $(event.currentTarget).attr("data-item-id");
            }
        }
    });
    
    // tlačítko hlavního menu - desktop, aktuálně otevřená stránka
    var opened = $(".section-navigation .mainmenu.mainmenu-level-1 > .mainmenu-button.opened");
    if (opened && opened[0]) {
        _this.openedDesktopId = parseInt(opened.attr("data-item-id"));
    }
    
    // panel submenu - desktop
    $(".mainmenu-open-cont").mouseenter(function(event){
        _this.removeDesktopTimer();
    });
    $(".mainmenu-open-cont").mouseleave(function(event){
        if (event.offsetY > 0) {
            _this.closeDesktopSubmenu();
        }
    });
    $("nav.nav-menu").mouseleave(function(event){
        _this.closeDesktopSubmenu();
    });
    
    // tlačítko zavřít submenu - desktop
    $(".mainmenu-open-cont .mainmenu-open-closebtn").click(function(event) {
        _this.closeDesktopSubmenu();
    }); 
    
    // reakce na změnu velikosti okna
    $(window).resize( function() {
        var width = $(window).width();
        if (width > _this.mobileWidth) {
            _this.closeMobileSubmenu();
        }
    });
    
    // reakce na posun obsahu
    //$(window).scroll(function(event) {
        //_this.closeMobileSubmenu();
    //});
};

/**
 * Přepne mobile submenu panel.
 * @param {Number} id ID panelu.
 */
mappengine.globalcontent.mainmenu.switchMobileSubmenu = function(id) {
    if ($(".mainmenu .mainmenu-button[data-item-id="+id+"] ul[data-item-id="+id+"]").is(':visible')) {
        this.closeMobileSubmenu();
    } else {
        this.openMobileSubmenu(id);
    }
};

/**
 * Otevře mobile submenu panel.
 * @param {Number} id ID panelu.
 */
mappengine.globalcontent.mainmenu.openMobileSubmenu = function(id) {
    this.currentMobileId = id;
    $(".mainmenu .mainmenu-button ul.mainmenu.mainmenu-level-2").hide();
    $(".mainmenu .mainmenu-button[data-item-id="+id+"] ul[data-item-id="+id+"]").show();
};

/**
 * Zavře mobile submenu panel.
 */
mappengine.globalcontent.mainmenu.closeMobileSubmenu = function() {
    if (this.currentMobileId) {
        this.currentMobileId = 0;
        $(".mainmenu .mainmenu-button ul.mainmenu.mainmenu-level-2").hide();
    }
};

/**
 * Spustí časovat pro otevření desktop submenu panelu.
 * @param {Number} id ID panelu.
 */
mappengine.globalcontent.mainmenu.startDesktopTimer = function(id) {
    var _this = mappengine.globalcontent.mainmenu;
    _this.removeDesktopTimer();
    if (_this.currentDesktopId) {
        _this.openTimer = setTimeout(function(){
            _this.openDesktopSubmenu(id);
        }, _this.openDelay);
    } else {
        _this.openDesktopSubmenu(id);
    }
};

/**
 * Odstraní časovat pro otevření desktop submenu panelu.
 */
mappengine.globalcontent.mainmenu.removeDesktopTimer = function() {
    if (this.openTimer) {
        clearTimeout(this.openTimer);
        this.openTimer = null;
    }
};

/**
 * Otevře desktop submenu panel.
 * @param {Number} id ID panelu.
 */
mappengine.globalcontent.mainmenu.openDesktopSubmenu = function(id) {
    var currentId = this.currentDesktopId;
    var panel = $(".mainmenu-open-cont .mainmenu-panel[data-item-id="+id+"]");
    if (panel.length > 0) {
        if (this.currentDesktopId !== id) {
            this.currentDesktopId = id;
            this.removeDesktopTimer();
            this.unhighlightDesktopOpenedItem();
            $(".mainmenu-open-cont .mainmenu-panel").hide();
            $(".mainmenu-open-cont .mainmenu-panel[data-item-id="+id+"]").show();
            $(".mainmenu-open-cont").fadeIn(100);
            $("li.mainmenu-button[data-item-id="+id+"] > a.mainmenu-item").addClass("selected");
            $("li.mainmenu-button[data-item-id="+currentId+"] > a.mainmenu-item").removeClass("selected");
        }
    } else {
        this.closeDesktopSubmenu();
    }
};

/**
 * Zavře desktop submenu panel.
 */
mappengine.globalcontent.mainmenu.closeDesktopSubmenu = function() {
    var currentId = this.currentDesktopId;
    if (this.currentDesktopId) {
        this.currentDesktopId = 0;
        this.removeDesktopTimer();
        this.highlightDesktopOpenedItem();
        $(".mainmenu-open-cont").fadeOut(100, function() {
            $(".mainmenu-open-cont .mainmenu-panel").hide();
            $("li.mainmenu-button[data-item-id="+currentId+"] > a.mainmenu-item").removeClass("selected");
        });
    }
};

/**
 * Vrací zda-li má položka desktop submenu panel, který má vlastní obsah.
 * Metoda netestuje pouze existenci panelu, ale také existenci obsahu v panelu.
 * @param {Number} id
 * @returns {Boolean}
 */
mappengine.globalcontent.mainmenu.hasDesktopSubmenu = function(id) {
    var _panel = $(".mainmenu-open-cont .mainmenu-panel[data-item-id="+id+"]");
    if (_panel && _panel.html() && _panel.html().trim() !== "") { 
        return true; 
    }
    return false;
};

/**
 * Nastaví zvýraznění otevřené položky 1 úrovně desktop menu.
 */
mappengine.globalcontent.mainmenu.highlightDesktopOpenedItem = function() {
    if (this.openedDesktopId) {
        $(".section-navigation .mainmenu.mainmenu-level-1 > .mainmenu-button[data-item-id="+this.openedDesktopId+"]").addClass("opened");
        $(".section-navigation .mainmenu.mainmenu-level-1 > .mainmenu-button[data-item-id="+this.openedDesktopId+"] > a").addClass("opened");
    }
};

/**
 * Zruší zvýraznění otevřené položky 1 úrovně desktop menu.
 */
mappengine.globalcontent.mainmenu.unhighlightDesktopOpenedItem = function() {
    if (this.openedDesktopId) {
        $(".section-navigation .mainmenu.mainmenu-level-1 > .mainmenu-button[data-item-id="+this.openedDesktopId+"]").removeClass("opened");
        $(".section-navigation .mainmenu.mainmenu-level-1 > .mainmenu-button[data-item-id="+this.openedDesktopId+"] > a").removeClass("opened");
    }    
};

$(document).ready(function(){
    mappengine.globalcontent.mainmenu.init();
});

/* ------------------------------------ /modules/globalcontent/globalcontent-web-form.js ------------------------------------ */


var mappengine = mappengine || {};

/**
 * Jmenný prostor pro webové JS API modulu Globalcontent určený pro výměnu obsahu.
 * @namespace
 */
mappengine.globalcontent = mappengine.globalcontent || {};

mappengine.globalcontent.form = {
    
    hashFilterId: "#filter",
    hashSeparator: "|",
    hashData: null,
    data: null,
    lastForm: null,
    lastContainer: null,
    alreadyInited: false,
    customAnchor: null,
    preloader: null,
    
    initOnReady: function() {
        $(window).on("hashchange", $.proxy( this.onHashChange, this ) );
        this.onHashChange();
        this.alreadyInited = true;
        this.preloader = new Image(120, 120);
        this.preloader.src = real_root_web+"/img/preloader.svg";
    },
    
    /**
     * Odesílání formuláře s výměnou kontejneru pro výpis 
     * @param {string} formName Jméno formuláře
     * @param {string} selectorHtmlContainer Selektor pro výměnu obsahu a zobrazení informace o načítání
     * @param {function} fncCheckValid(form) Kontrola formuláře pro validní vstup - nemusí být zadána
     * @param {boolean} disableHash
     * @param {string} customAnchor Selektor pro odscrolování obsahu
     */
    changeContent: function( formName, selectorHtmlContainer, fncCheckValid, disableHash, customAnchor) {
        if( fncCheckValid && !fncCheckValid( document.forms[formName] ) ) {
            return;
        }
        var form = this.lastForm = $( document.forms[formName] );
        var containter = this.lastContainer = ( $(selectorHtmlContainer) );
        var data = this.data = form.serializeArray();
        
        // vytvoreni hash pro odeslani formulare
        var hashData = [];
        hashData.push( this.hashFilterId );
        hashData.push( encodeURIComponent( formName ) );
        hashData.push( encodeURIComponent( selectorHtmlContainer ) );
        hashData.push( encodeURIComponent( JSON.stringify( data ) ) );

        this.customAnchor = customAnchor;
        
        if( disableHash ) {
            this.onHashChange( data );
        } else {
            location.hash = this.hashData = hashData.join( this.hashSeparator );
        }    
    },

    /**
     * Zobrazení informace o nahrávání
     * @param {type} containter
     * @returns {undefined}
     */
    showLoading: function( containter ) {
        containter.css("position","relative");
        var text = "Výpis se aktualizuje...";
        if( current_lang === "en" ) {
            text = "The listing is updated...";
        }
        containter.append( "<div class='loading'><div class='preloaderText'>"+text+"</div><div class='preloaderImg'></div>" );
        containter.find(".preloaderImg").append(this.preloader);
    },

    /**
     * Komunikace se server selhala
     * @param {type} containter
     * @returns {undefined}
     */
    failed: function( containter ) {
        containter.css("position","");
        containter.children(".loading").remove();

        var msg = "";
        switch (current_lang) {
            case "cz":
                msg = "Komunikace se server selhala!\nOpakujte akci.";
                break;
            default:
                msg = "Lost connection!\nPlease, try again.";
                break;
        }
        alert( msg );
    },

    /**
     * Zobrazení načteného obsahu
     * @param {type} containter
     * @param {type} data
     * @returns {undefined}
     */
    change: function( containter, data ) {
        containter.css("position","");
        containter.html( $(data.html).contents() );
        if( data.contentHideSelector ) {
          this.contentHide( data.contentHideSelector );
        }
        if( data.contentShowSelector ) {
            this.contentShow( data.contentShowSelector );
        }
        mappengine.globalcontent.tablesort && mappengine.globalcontent.tablesort.Init();
    },
    
    onHashChange: function( newData ) {
        
        if( location.hash.length == 0 && arguments.length == 0 ) {            
            return;
        }
        
        var hashData = location.hash.split( this.hashSeparator );
        
        if ( arguments.length == 0 && hashData[0] !== this.hashFilterId ) {
            return;
        }
        
        
        var form, 
            containter,
            hashEmpty = hashData.length == 1 && hashData[0] == "";
        
        if ( hashData.length == 4 ) {
            var formName = decodeURIComponent( hashData[1] );
            var selectorHtmlContainer = decodeURIComponent( hashData[2] );
            form = $( document.forms[formName] );
            containter = ( $(selectorHtmlContainer) );
        } else if( hashEmpty ) {            
            form = this.lastForm;
            containter = this.lastContainer;
        } else {
            return;
        }
        
        // posun na výsledek filtru
        var menuTopHeight = 48 + 80; // vyska menu na desktopu
        if( $( window ).width() <= 991 ) {
            menuTopHeight = 0 + 80;
        }
        
        if (this.customAnchor) {
            $( document ).scrollTop( $(this.customAnchor).offset().top - menuTopHeight);
        } else {
            $( document ).scrollTop( containter.offset().top - menuTopHeight);
        }
        
        
        this.showLoading( containter );
        
        var data = null;
        if( this.hashData == location.hash || this.data == newData ) {
            data = this.data;
        } else {
            if( hashEmpty ) {
                form[0].reset();
                var reg = form[0].elements["regions"];
                if( reg ) reg.value = "";
            } else {
                data = JSON.parse( decodeURIComponent( hashData[3] ) );
                form[0].reset();
                form.deserialize( data );
            }
        }
        this.updateRegions( data, form );
                
        $.ajax({
                type: form.attr('method'),
                url: form.attr('action'),
                data: form.serialize(),
                cache: false
        })
            .done( $.proxy(this.change, this, containter ) )
            .fail( $.proxy(this.failed, this, containter ) );

    },
    
    updateRegions: function( data, form ) {
        if ( data ) {
            var filtered = data.filter( function( item ) {
               return item["name"] == "regions";
            });
            if( filtered.length == 1 ) {
                mappengine.globalcontent.filterMap.UpdateRegionVisibility( filtered[0]["value"].split(",") );
            } else {
                mappengine.globalcontent.filterMap.UpdateRegionVisibility( [] );
            }
        } else {
            mappengine.globalcontent.filterMap.UpdateRegionVisibility( [] );
        }
    },

    contentHide: function( selector ) {
      $(selector).hide();
    },

    contentShow: function( selector ) {
      $(selector).show();
    }
       
}

$(document).ready( $.proxy(mappengine.globalcontent.form.initOnReady, mappengine.globalcontent.form ) );


/* ------------------------------------ /modules/globalcontent/globalcontent-web-filter-map.js ------------------------------------ */


var mappengine = mappengine || {};

/**
 * Jmenný prostor pro webové JS API modulu Globalcontent určený pro výměnu obsahu.
 * @namespace
 */
mappengine.globalcontent = mappengine.globalcontent || {};

mappengine.globalcontent.filterMap = {
    
    hiddenClass: "layoutHidden",
    
    Init: function() {
        $(".filterMapaActive")
                .on("click", $.proxy( this.ChangeVisibility, this ) )
                .hover( $.proxy( this.ChangeHoverInOut, this ) )
        ;
    },
    
    ChangeVisibility: function( event ) {
        event.preventDefault();        
        var target = event.currentTarget;
        var id = $(target).attr("data-id");
        
        var layout = $(target.hash);
        
        if ( id.length > 0 ) { 
            if( layout.hasClass( this.hiddenClass ) ) {
                this.FindTargetWithInputInForm( target, "input[name=regions]", $.proxy( this.IdAdd, this, id ) );
            } else {
                this.FindTargetWithInputInForm( target, "input[name=regions]", $.proxy( this.IdRemove, this, id ) );
            }
        }
        
        layout.toggleClass( this.hiddenClass );
    },
    
    ChangeHoverInOut: function( event ) {
        var target = event.currentTarget;
        $(target.hash).toggleClass("hover");
    },
    
    FindTargetWithInputInForm: function( target, inputSelector, callback ) {
        
        for ( var i = 0, len = document.forms.length; i < len; i++ ) {
            var elem = $( document.forms[i] );
            if ( elem.find( $(target) ).length > 0 ) {
                var input = elem.find( inputSelector );
                if( input.length > 0 ) {                    
                    callback( input );
                }
            }
        }
    },
    
    IdAdd: function( id, elem ) {        
        if( elem.length == 0 ) {
            return;
        }
        var v = elem.val().split( "," );
        if( v[0] === "" ) {
            v = [];
        }
        v.push( id );
        v = $.unique( v );
        elem.val( v.join( "," ) );
    },
    
    IdRemove: function( id, elem ) {
        if( elem.length == 0 ) {
            return;
        }
        var v = elem.val().split( "," );
        if( v[0] === "" ) {
            v = [];
        }
        var idx = v.indexOf( id );
        if ( idx > -1 ) {
            v.splice( idx, 1);
        }
        elem.val( v.join( "," ) );
    },
    
    UpdateRegionVisibility: function( regionsIds ) {
        var hiddenClass = this.hiddenClass;
        $(".filterMapaActive").each( function(i, elem ) {
            var item = $(elem);
            if( regionsIds.indexOf( item.attr("data-id") ) == -1 ) {
                $(elem.hash).addClass( hiddenClass );
            } else {
                $(elem.hash).removeClass( hiddenClass );
            }
        } );
    }
}

$(document).ready( $.proxy( mappengine.globalcontent.filterMap.Init, mappengine.globalcontent.filterMap ) );

/* ------------------------------------ /inc/select2/select2-4.0.3/js/select2.min.js ------------------------------------ */

