function changeOdds(a){var b=$("#match-main-menu li.active-tab").attr("id");reloadSubmenuFromJson(b,a)}function changeMainTab(a){a.preventDefault(),clearTimeout($timeout);var b=$(this).parent("li");b.hasClass("menuMatchStream")?showStreamPanel(b):showAdditionalPanel(b,b.attr("id"),null)}function changeSubmenuTab(){clearTimeout($timeout);var a=$(this).parent("li"),b=a.attr("id");loadAdditionalInfo(a,b)}function showStreamPanel(a){$("#match-additional").hide(),$("#match-stream").show(),activateMainTab(a),window.location.hash="menuMatchStream",$("#match-submenu-additional").hide(),$("#match-submenu").show()}function showAdditionalPanel(a,b,c){$("#match-stream").hide(),$("#match-additional").show(),activateMainTab(a),"otherSports"===matchJson.sport&&"menuMatchTips"===b?reloadSubmenuFromJson(b,c):reloadSubmenu(b,c),$("#match-submenu").hide(),$("#match-submenu-additional").show()}function activateMainTab(a){$("#match-main-menu .active-tab").removeClass("active-tab"),$(a).addClass("active-tab")}function activateSubmenuTab(a){$("#match-submenu-additional .tabSelected").removeClass("tabSelected"),$(a).addClass("tabSelected")}function reloadSubmenu(a,b){$("#match-submenu-additional").empty();var c=submenu[matchJson.sport][a].items,d="";$.each(c,function(a,b){d+="<li class='tab' id='"+a+"'><a href='javascript:void(0)'>"+b.label+"</a></li>"}),$("#match-submenu-additional").html(d);var e,f;void 0!==b&&null!==b&&b.length?(f=b,e=$("#match-submenu-additional li#"+b)):(e=$("#match-submenu-additional li:first"),f=$("#match-submenu-additional li:first").attr("id")),loadAdditionalInfo(e,f)}function reloadSubmenuWithoutLoop(a,b){$("#match-submenu-additional").empty();var c=submenu[matchJson.sport][a].items,d="";$.each(c,function(a,b){d+="<li class='tab' id='"+a+"'><a href='javascript:void(0)'>"+b.label+"</a></li>"}),$("#match-submenu-additional").html(d);var e,f;void 0!==b&&null!==b&&b.length?(f=b,e=$("#match-submenu-additional li#"+b)):(e=$("#match-submenu-additional li:first"),f=$("#match-submenu-additional li:first").attr("id"))}function reloadSubmenuFromJson(a,b){$("#match-submenu-additional").empty();var c=submenu[matchJson.sport][a],d=config.JSONserver[lang]+c.submenuJsonSource+matchJson.id+"_"+lang.toUpperCase()+".json",e=$.getJSON(d);e.done(function(a){var d=a.sub_events,e="";$.each(d,function(a,b){e+="<li class='tab' id='oddEvent"+a+"' data-odd-event='"+a+"'><a class='tab-special tab-other-odds' href='javascript:void(0)'>"+b.name+"</a></li>"}),$("#match-submenu-additional").html(e);var f,g;void 0!==b&&null!==b&&b.length?(g=b,f=$("#match-submenu-additional li#"+b)):(f=$("#match-submenu-additional li:first"),g=$("#match-submenu-additional li:first").attr("id")),activateSubmenuTab(f),loadOtherOdds(c,a,g)}).fail(function(){notavailable(a)})}function loadOtherOdds(a,b,c){var d=$("#match-main-menu li.active-tab").attr("id");window.location.hash="m:"+d+";p:"+c;var e=getTemplate(a.template);e.done(function(a){var d=htmltemplate(a),e=d.find("thead"),f=d.find("tbody"),g=$("#"+c).data("odd-event"),h=b.sub_events[g],i=b.betting_offices,j=$("<tr><td class='odd-name'></td></tr>");$.each(i,function(a,b){$(".odd-images",e).append("<th class='odd-n odd-bo-"+b.id+"'><div class='odd-image'><img width='19' height='90' alt='"+b.name+"' src='"+imgBase+"betoffices/vertical/"+b.id+".png'></div></th>");var c=null===b.international_link?b.url:b.international_link,d=$("<td class='odd-n odd-bo-"+b.id+"'><span></span></td>").attr("onclick","window.open('"+c+"', '_blank')").attr("title",t("betOn")+" "+b.name+" "+t("newWindow"));j.append(d)}),j.append("<td class='odd-empty'></td>"),$(".odd-images",e).append("<th class='odd-empty'></th>"),$.each(h.items,function(a,b){var c=j.clone();$(".odd-name",c).text(b.name),null!==b.name.match(/\(CZE\)/)&&$(".odd-name",c).prepend("<img src='"+imgBase+"flag_cs.jpg' style='margin: 0 4px 0 0;' alt='CZ' />");var e=0;$.each(b.courses,function(a,b){var f;if(""!==b.value){g=b.value.replace(",","."),f=parseFloat(g);var h=parseFloat(e);f>=h